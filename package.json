{
  "name": "deploy-sandbox-schema",
  "version": "0.1.0",
  "description": "Postgresql deployment automation - sandbox",
  "scripts": {
    "migrate:status": "node src/migrate status",
    "migrate:latest": "node src/migrate latest",
    "migrate:create": "node src/migrate create",
    "migrate:drop": "node src/migrate drop",
    "migrate:recreate": "npm run migrate:drop && npm run migrate:create && npm run migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsthorup/deploy-sandbox-schema.git"
  },
  "author": "Lars Thorup <lars@zealake.com> (http://github.com/larsthorup)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larsthorup/deploy-sandbox-schema/issues"
  },
  "homepage": "https://github.com/larsthorup/deploy-sandbox-schema#readme",
  "dependencies": {
    "cryptex": "^0.6.1",
    "knex": "^0.10.0",
    "pg": "^4.5.3"
  }
}
